{
    "indentSize": 4,
    "wrapAttributes": "force-expand-multiline",
    "wrapAttributesMinAttrs": 3,
    "wrapLineLength": 120,
    "endWithNewLine": true,
    "noMultipleEmptyLines": false,
    "useTabs": false,
    "sortTailwindcssClasses": false,
    "sortHtmlAttributes": "none",
    "noPhpSyntaxCheck": false,
    "noSingleQuote": false,
    "noTrailingCommaPhp": false,
    "componentPrefix": [
        "x-",
        "livewire:"
    ],
    "phpVersion": "8.4"
}